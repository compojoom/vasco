<?xml version="1.0" encoding="utf-8" ?>
<extension type="module" version="2.5" method="upgrade" client="site">
	<name>@@MODULENAME@@</name>
	<creationDate>@@DATE@@</creationDate>
	<author>Compojoom</author>
	<copyright>(C) 2014 Compojoom</copyright>
	<license>http://www.gnu.org/licenses/gpl.html</license>
	<authorEmail>yves@compojoom.com</authorEmail>
	<authorUrl>http://compojoom.com</authorUrl>
	<version>@@VERSION@@</version>
	<description>Connects your Joomla Backend with the MYDIGIPASS</description>
	<files>
		##PACKAGEFILESMODULE##
	</files>
	<languages folder="language">
		##FRONTENDLANGUAGEFILES##
	</languages>
	<media destination="@@MODULENAME@@" folder="media/@@MODULENAME@@">
		##MEDIAPACKAGEFILES##
	</media>
	<config>
		<fields name="params">
			<fieldset name="basic">
			</fieldset>
			<fieldset name="advanced">

				<field name="moduleclass_sfx" type="text" default="" label="MOD_MATUKIO_UPCOMING_CLASS_SUFFIX"
				       description="MOD_MATUKIO_UPCOMING_SUFFIX_DESC"/>

			</fieldset>
		</fields>
	</config>
</extension>

